<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PlaceProfileActivity">

    <Button
        android:id="@+id/button8"
        android:layout_width="100dp"
        android:layout_height="45dp"
        android:text="Block"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="279dp"
        tools:layout_editor_absoluteY="105dp" />

    <Button
        android:id="@+id/button7"
        android:layout_width="100dp"
        android:layout_height="45dp"
        android:backgroundTint="#005CE5"
        android:text="Follow"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="279dp"
        tools:layout_editor_absoluteY="60dp" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recycler_view"
        android:layout_width="367dp"
        android:layout_height="195dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/interestText2" />

    <TextView
        android:id="@+id/interestText2"
        android:layout_width="365dp"
        android:layout_height="48dp"
        android:layout_marginTop="12dp"
        android:text="interests:"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/bioText2"
        app:layout_constraintTop_toBottomOf="@+id/bioText2"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/ratingText2"
        android:layout_width="118dp"
        android:layout_height="30dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="16dp"
        android:text="rating"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/profilePicture2"
        app:layout_constraintTop_toBottomOf="@+id/nickNameText2"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/ratingText6"
        android:layout_width="118dp"
        android:layout_height="30dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="16dp"
        android:text="followers"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/profilePicture2"
        app:layout_constraintTop_toBottomOf="@+id/ratingText2"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/bioText2"
        android:layout_width="365dp"
        android:layout_height="59dp"
        android:text="bio"
        android:textSize="16sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="151dp" />

    <ImageView
        android:id="@+id/profilePicture2"
        android:layout_width="117dp"
        android:layout_height="119dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/ic_baseline_person_24"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/nickNameText2"
        android:layout_width="190dp"
        android:layout_height="33dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="16dp"
        android:text="place name"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/profilePicture2"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>